import SimpleJson


data Doc = ToBeDefined
          deriving (Show)


string :: String -> Doc
string str = Undefined


(<>) :: Doc -> Doc -> Doc
a <> b = undefined

char :: Char -> Doc
char c = undefined


text :: String -> Doc
text str = Undefined

double :: Double -> Doc
double num = undefined

hcat :: [Doc] -> Doc
hcat xs = undefined

fsep :: [Doc] -> Doc
fsep xs = undefined
